<schemalist>
	<schema gettext-domain="@GETTEXT_PACKAGE@" id="com.timxx.cnweather" path="/apps/cnweather/">
		<key name="window-size" type="(ii)">
			<default>(500,300)</default>
			<_summary>Window size</_summary>
			<_description>The width and height of window</_description>
		</key>
		<key name="window-pos" type="(ii)">
			<default>(-1,-1)</default>
			<_summary>Window position</_summary>
			<_description>The last window position</_description>
		</key>
		<key name="window-maximized" type="b">
			<default>false</default>
			<_summary>Is window maximized</_summary>
			<_description>The last state of window</_description>
		</key>
		<key name="theme" type="s">
			<default>''</default>
			<_summary>Icon theme for weather</_summary>
		</key>
		<key name="city-id" type="as">
			<default>[]</default>
			<_summary>list of city id</_summary>
			<_description>city ids for weather information</_description>
		</key>
		<key name="city" type="as">
			<default>[]</default>
		</key>
		<key name="duration" type="i">
			<default>30</default>
			<_summary>Refresh duration</_summary>
			<_description>Duration to refresh weather</_description>
		</key>
		<key name="tray" type="b">
			<default>false</default>
			<_summary>System Tray</_summary>
			<_description>Show system tray or not</_description>
		</key>
		<key name="weather1" type="as">
			<default>[]</default>
		</key>
		<key name="wind1" type="as">
			<default>[]</default>
		</key>
		<key name="temp1" type="as">
			<default>[]</default>
		</key>
		<key name="img1" type="as">
			<default>[]</default>
		</key>
		<key name="weather2" type="as">
			<default>[]</default>
		</key>
		<key name="wind2" type="as">
			<default>[]</default>
		</key>
		<key name="temp2" type="as">
			<default>[]</default>
		</key>
		<key name="img2" type="as">
			<default>[]</default>
		</key>

		<key name="weather3" type="as">
			<default>[]</default>
		</key>
		<key name="wind3" type="as">
			<default>[]</default>
		</key>
		<key name="temp3" type="as">
			<default>[]</default>
		</key>
		<key name="img3" type="as">
			<default>[]</default>
		</key>
		<key type="as" name="temp">
			<default>[]</default>
			<_summary>real time temperature</_summary>
			<_description>real time temperature for current day</_description>
		</key>
	</schema>
</schemalist>
